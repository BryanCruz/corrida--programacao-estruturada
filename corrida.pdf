	<body onload="if ((document.getElementById('pw').passwordfocus != true)) document.getElementById('eid').focus() ;parent.updCourier(doubleDeep, ignoreCourier);">
		<script type="text/javascript" language="JavaScript">
			focus_path = ["eid"];
		</script>
		
		<!--ufabc-famt: script para ocultar div contendo o campo de login; usado para impedir login durante manutenção do sistema-->
		<script>
			function toggle(obj) {
				var el = document.getElementById(obj);
				if ( el.style.display != 'none' ) {
				el.style.display = 'none';
			}else {
				el.style.display = '';
				}
			}
		</script>
		
		<!-- xlogin.vm -->
		<!--ufabc-famt: mensagem orientando loga sempre com login institucional-->
		<!-- <a href="#" onclick="toggle('maisinfo');">.</a>
